{
  "expo": {
    "sdkVersion": "38.0.0",
    "name": "UdaciCards",
    "slug": "UdaciCards",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "useNextNotificationsApi": true,
      "enableDangerousExperimentalLeanBuilds": true,
      "package": "org.udacicards.exp",
      "splash": {
        "mdpi": "./assets/drawable-mdpi/splash.png",
        "hdpi": "./assets/drawable-hdpi/splash.png",
        "xhdpi": "./assets/drawable-xhdpi/splash.png",
        "xxhdpi": "./assets/drawable-xxhdpi/splash.png",
        "xxxhdpi": "./assets/drawable-xxxhdpi/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.udacicards.exp",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "icon": "./assets/favicon.png"
    },
    "description": "Udacity React Nanodegree Final Project",
    "githubUrl": "https://github.com/dhatGuy/reactnd-UdaciCards"
  }
}
